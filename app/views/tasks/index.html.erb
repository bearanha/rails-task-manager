<h1>These are your tasks:</h1>
  <ul style="list-style: none">
    <% @tasks.each do |task| %>
      <div class="card">
        <li style="text-decoration: none" ><%= link_to task.title, task_path(task) %></li>
      </div>
    <% end %>
    </ul>

    <%= link_to "Create new task", new_task_path %>
